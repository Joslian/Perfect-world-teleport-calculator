<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="TranslationSchema" targetNamespace="http://TeleportCalculator/TranslationSchema.xsd" elementFormDefault="qualified" xmlns="http://TeleportCalculator/TeleportSchema.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Translations">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Translation" maxOccurs ="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ID" type="xs:unsignedShort" />
              <xs:element name="Name" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence >
      <xs:attribute name ="Version" type ="xs:string "/>
      <xs:attribute name ="Culture" type ="xs:string "/>
    </xs:complexType >
  </xs:element >
</xs:schema>